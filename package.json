{
  "name": "rheactor-web-app",
  "description": "Contains the building blocks for a web application",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "environment=testing mocha --reporter=dot --timeout=1350 test/\\*\\*/\\*.js",
    "test:coverage": "environment=testing istanbul cover _mocha --report lcovonly -- -R dot --timeout=1350 test/\\*\\*/\\*.js && codeclimate-test-reporter < ./coverage/lcov.info && rm -rf ./coverage",
    "precommit": "npm run lint",
    "commitmsg": "validate-commit-msg"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ResourcefulHumans/rheactor-web-app.git"
  },
  "author": "Resourceful Humans <info@resourceful-humans.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ResourcefulHumans/rheactor-web-app/issues"
  },
  "homepage": "https://github.com/ResourcefulHumans/rheactor-web-app#readme",
  "dependencies": {
    "angular": "^1.6.1",
    "bluebird": "^3.4.6",
    "bootstrap": "^4.0.0-alpha.5",
    "compare-versions": "^3.0.0",
    "event-source-polyfill": "0.0.7",
    "lodash": "^4.15.0",
    "moment": "^2.17.1",
    "rheactor-value-objects": "^3.6.0",
    "showdown": "^1.5.5"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.1.1",
    "babel-preset-es2015": "^6.18.0",
    "babel-register": "^6.18.0",
    "chai": "^3.5.0",
    "codeclimate-test-reporter": "^0.4.0",
    "husky": "<2.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "mocha-lcov-reporter": "^1.2.0",
    "semantic-release": "^6.3.2",
    "standard": "^8.6.0",
    "validate-commit-msg": "^2.8.2"
  },
  "files": [
    "dist",
    "img",
    "includes",
    "scss",
    "README.md"
  ]
}
